{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "node": false
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-inner-declarations": 0,
        "no-restricted-syntax": 0,
        "no-plusplus": 0,
        "no-bitwise": 0,
        "no-underscore-dangle": 0,
        "camelcase": 0,
        "no-continue": 0,
        "no-use-before-define": 0
    }
}
